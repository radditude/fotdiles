# Stuff I don't want in VCS goes here
[include]
path = ~/.gitconfig.local
[push]
default = simple
[core]
editor = vim
excludesfile = /Users/cjhorton/.gitignore
pager = less -F -X
[hub]
protocol = https
[color]
diff = auto
status = auto
branch = auto
ui = true
[alias]
co = checkout
nb = checkout -b
new = !git init && git checkout -b main
[pull]
rebase = false
[init]
defaultBranch = main
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
