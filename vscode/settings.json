{
  "editor.fontFamily": "Fira Code, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "eslint.run": "onSave",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.checkoutType": "local",
  "git.countBadge": "tracked",
  "git.decorations.enabled": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.external.osxExec": "iTerm",
  "workbench.colorTheme": "Oceanic Plus",
  "workbench.iconTheme": "file-icons",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#6699CC",
    "badge.background": "#6699CC",
    "editorError.foreground": "#EC5F67",
    "editorWarning.foreground": "#EC5F67"
  },
  "window.zoomLevel": 0,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.views.repositories.files.layout": "tree",
  "gitlens.views.fileHistory.enabled": false,
  "gitlens.views.lineHistory.enabled": false,
  "[yaml]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.lineHeight": 20,
  "editor.wordWrap": "wordWrapColumn",
  "[markdown]": {
    "editor.wordWrapColumn": 80,
    "editor.wordWrap": "wordWrapColumn",
    "files.trimTrailingWhitespace": false,
    "files.insertFinalNewline": false,
    "editor.trimAutoWhitespace": false
  },
  "markdown.format.proseWrap": "never",
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": false,
  "breadcrumbs.enabled": true,
  "liveshare.featureSet": "insiders",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search-node-modules.path": "node_modules",
  "files.associations": {
    "*.leex": "Eex",
    "*.symlink": "shellscript",
    "*.erb": "html",
    "*.md": "markdown"
  },
  "javascript.suggest.autoImports": false,
  "yaml.format.singleQuote": true,
  "editor.autoClosingQuotes": "always",
  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": {
      "useBundler": false
    }
  },
  "ruby.codeCompletion": "rcodetools",
  "ruby.intellisense": "rubyLocate",
  "[ruby]": {
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "indentRainbow.colors": [
    "rgba(136, 176, 215, 0.07)",
    "rgba(252, 218, 149, 0.04)",
    "rgba(165, 205, 160, 0.04)",
    "rgba(197, 148, 197, 0.04)"
  ],
  "terraform.indexing": {
    "enabled": true,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "terraform.languageServer": {
    "enabled": false,
    "args": []
  },
  "shellformat.path": "/usr/local/bin/shfmt",
  "shellformat.flag": "-sr",
  "html.format.wrapLineLength": 0,
  "markdown.preview.fontFamily": "'Menlo', 'Droid Sans', 'Arial', sans-serif",
  "markdown.preview.lineHeight": 1.8,
  "markdown.preview.fontSize": 15,
  "markdown.preview.openMarkdownLinks": "inEditor",
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.toc.githubCompatibility": true,
  "prettier.proseWrap": "always",
  "editor.wordWrapColumn": 120,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "go.formatTool": "goimports",
  "prettier.configPath": ".prettierrc",
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/vendor/**": true,
    "**/yarn_cache/**": true,
    "**/dist/**": true
  },
  "search.exclude": {
    "**/tmp": true,
    "**/vendor": true,
    "**/yarn_cache": true
  },
  "html.format.indentHandlebars": true,
  "html.format.enable": false,
  "workbench.editor.showTabs": true,
}
